
# Default: Domain C standart without
echo "test" > results.tsv
lengte=$(cat results.tsv | wc -l)

while  [ $lengte -lt 2 ]
do
	python3 multi_momma.py -t files/mouseratblast -g files/goa_rat.gaf -T files/mousedata -G files/goa_mouse.gaf -s 10 -p predictors/pred_blast_besthit.py -r 1 -e f-score -d C
        lengte=$(cat results.tsv | wc -l)
done


# Domain only biological
echo "test" > results.tsv
lengte=$(cat results.tsv | wc -l)

while  [ $lengte -lt 2 ]
do
	python3 multi_momma.py -t files/mouseratblast -g files/goa_rat.gaf -T files/mousedata -G files/goa_mouse.gaf -s 10 -p predictors/pred_blast_besthit.py -r 1 -e f-score -d F,C
        lengte=$(cat results.tsv | wc -l)
done


# Domain only molecular
echo "test" > results.tsv
lengte=$(cat results.tsv | wc -l)

while  [ $lengte -lt 2 ]
do
	python3 multi_momma.py -t files/mouseratblast -g files/goa_rat.gaf -T files/mousedata -G files/goa_mouse.gaf -s 10 -p predictors/pred_blast_besthit.py -r 1 -e f-score -d P,C
        lengte=$(cat results.tsv | wc -l)
done


# filtered evidence
echo "test" > results.tsv
lengte=$(cat results.tsv | wc -l)

while  [ $lengte -lt 2 ]
do
	python3 multi_momma.py -t files/mouseratblast -g files/goa_rat.gaf -T files/mousedata -G files/goa_mouse.gaf -s 10 -p predictors/pred_blast_besthit.py -r 1 -e f-score -d C -E IEA
        lengte=$(cat results.tsv | wc -l)
done

# Filtered evidence and domain biological
echo "test" > results.tsv
lengte=$(cat results.tsv | wc -l)

while  [ $lengte -lt 2 ]
do
	python3 multi_momma.py -t files/mouseratblast -g files/goa_rat.gaf -T files/mousedata -G files/goa_mouse.gaf -s 10 -p predictors/pred_blast_besthit.py -r 1 -e f-score -d C,F -E IEA
        lengte=$(cat results.tsv | wc -l)
done

# Filtered evidence and domain Molecular
echo "test" > results.tsv
lengte=$(cat results.tsv | wc -l)

while  [ $lengte -lt 2 ]
do
	python3 multi_momma.py -t files/mouseratblast -g files/goa_rat.gaf -T files/mousedata -G files/goa_mouse.gaf -s 10 -p predictors/pred_blast_besthit.py -r 1 -e f-score -d C,P -E IEA
        lengte=$(cat results.tsv | wc -l)
done
