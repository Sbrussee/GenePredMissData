# Default: 
# domain C wordt als default eruit gelaten
python3 multi_momma.py -t files/mouseratblast -g files/goa_rat.gaf -T files/mousedata -G files/goa_mouse.gaf -s 10 -p predictors/pred_blast_besthit.py -r 3 -n 1 -e f-score
python3 multi_momma.py -t files/mouseratblast -g files/goa_rat.gaf -T files/mousedata -G files/goa_mouse.gaf -s 10 -p predictors/pred_blast_besthit.py -r 3 -n 1 -e precision

# Filteren op domains:
# P (biological process), F (molecular function) or C (cellular component).
python3 multi_momma.py -t files/mouseratblast -g files/goa_rat.gaf -T files/mousedata -G files/goa_mouse.gaf -s 10 -p predictors/pred_blast_besthit.py -d P -n 1 -r 3 -e f-score
python3 multi_momma.py -t files/mouseratblast -g files/goa_rat.gaf -T files/mousedata -G files/goa_mouse.gaf -s 10 -p predictors/pred_blast_besthit.py -d P -n 1 -r 3 -e precision
python3 multi_momma.py -t files/mouseratblast -g files/goa_rat.gaf -T files/mousedata -G files/goa_mouse.gaf -s 10 -p predictors/pred_blast_besthit.py -d F -n 1 -r 3 -e f-score
python3 multi_momma.py -t files/mouseratblast -g files/goa_rat.gaf -T files/mousedata -G files/goa_mouse.gaf -s 10 -p predictors/pred_blast_besthit.py -d F -n 1 -r 3 -e precision


# Filteren op evidence
python3 multi_momma.py -t files/mouseratblast -g files/goa_rat.gaf -T files/mousedata -G files/goa_mouse.gaf -s 10 -p predictors/pred_blast_besthit.py -E IEA -n 1 -r 3 -e f-score
python3 multi_momma.py -t files/mouseratblast -g files/goa_rat.gaf -T files/mousedata -G files/goa_mouse.gaf -s 10 -p predictors/pred_blast_besthit.py -E IEA -n 1 -r 3 -e precision


